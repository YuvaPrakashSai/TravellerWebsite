{
  "version": 2,
  "functions": {
    "api/wsgi.py": {
      "runtime": "@vercel/python",
      "maxDuration": 30,
      "memory": 1024
    }
  },
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/wsgi.py"
    }
  ]
}
