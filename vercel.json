{
  "version": 2,
  "functions": {
    "TravellerWebsite/wsgi.py": {
      "runtime": "python3.9",
      "maxDuration": 30,
      "memory": 1024
    }
  },
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "TravellerWebsite/wsgi.py"
    }
  ]
}
